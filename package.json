{
  "version": "1.0.0",
  "name": "express-error-log-handler",
  "description": "Express middleware-like error handler",
  "main": "lib/express-error-log-handler.js",
  "module": "lib/express-error-log-handler.es.js",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write \"src/**/*.js\"",
    "build": "rimraf lib && rollup --config",
    "prepublish": "npm run build"
  },
  "keywords": [
    "express",
    "log",
    "error",
    "handler",
    "middleware"
  ],
  "author": {
    "name": "Mike Chatsky",
    "email": "mike20016@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/mikeChatsky/express-error-log-handler#express-error-log-handler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeChatsky/express-error-log-handler.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.(test|spec)\\.[jt]sx?$"
  },
  "dependencies": {
    "in-production": "^1.0.1",
    "statuses": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-jest": "^24.7.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "lint-staged": "^13.1.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0",
    "sinon": "^7.3.2"
  }
}
